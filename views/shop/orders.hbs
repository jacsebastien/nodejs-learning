<h1>Your orders</h1>
<ul>
    {{#each orders}}
        <li>
            <h2>#{{this.id}}</h2>
            <ul>
                {{#each this.products}}
                    <li>{{this.title}} ({{this.orderItem.quantity}})</li>
                {{/each}}
            </ul>
        </li>
    {{else}}
        <li>No orders</li>
    {{/each}}
</ul>